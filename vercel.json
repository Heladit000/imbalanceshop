{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/docs",
      "dest": "public/docs/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "public/docs/$1"
    }
  ]
}
